<odoo>
    <data>
        <template id="custom_js_website" inherit_id="web.assets_frontend" name="Custom add cart check">
            <xpath expr="." position="inside">
                <script type="text/javascript" src="/website_custom_js/static/js/custom.js"></script>
            </xpath>
        </template>

        <template id="add_cart_limited_quantity">
            <t t-call="web.html_container">
                <t t-call="website.layout">
                    <div class="container oe_structure mb32 mt32">
                        <h2><b> You reached your monthly quantity of this product</b></h2>
                    </div>
                </t>
            </t>
        </template>
    </data>
</odoo>